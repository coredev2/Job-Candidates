datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Candidate {
  id         Int     @id @default(autoincrement())
  firstName  String
  lastName   String
  phone      String?
  email      String  @unique
  callTime   String?
  linkedIn   String?
  gitHub     String?
  comment    String
}